<head>
    <title>{{title}}</title>
</head>
       <div id="container" class="container">
    <div align="center">
  {{# if error}}
   <div class="alert alert-danger">
    <p> {{error}}</p>
    </div>
{{/if}} 

{{#if errors}}
      {{#each errors}}
        <div class="alert alert-danger"><p>{{msg}}</p> </div>
      {{/each}}
{{/if}}
{{# if success}}
  <div class="alert alert-success">
    <p> {{success}}</p>
    </div>
{{/if}} 

{{# if success_msg}}
  <div class="alert alert-success">
    <p> {{success_msg}}</p>
    </div>
{{/if}} 
</div>
</div>
<h2>Home</h2>